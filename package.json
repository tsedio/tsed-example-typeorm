{
  "name": "tsed-example-typeorm",
  "version": "1.0.0",
  "description": "Here an example project with TypeORM and Ts.ED framework.",
  "scripts": {
    "build": "yarn run barrels && yarn run tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "yarn run barrels && tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn run test:lint && yarn run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn run test:unit",
    "typeorm": "typeorm-ts-node-commonjs",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@tsed/ajv": "^6.114.2",
    "@tsed/common": "^6.114.2",
    "@tsed/core": "^6.114.2",
    "@tsed/di": "^6.114.2",
    "@tsed/engines": "^6.114.2",
    "@tsed/exceptions": "^6.129.0",
    "@tsed/json-mapper": "^6.114.2",
    "@tsed/logger": "^6.1.1",
    "@tsed/logger-file": "^6.1.1",
    "@tsed/passport": "^6.114.2",
    "@tsed/platform-cache": "^6.114.2",
    "@tsed/platform-exceptions": "^6.114.2",
    "@tsed/platform-express": "^6.114.2",
    "@tsed/platform-log-middleware": "^6.114.2",
    "@tsed/platform-middlewares": "^6.114.2",
    "@tsed/platform-params": "^6.114.2",
    "@tsed/platform-response-filter": "^6.114.2",
    "@tsed/platform-views": "^6.114.2",
    "@tsed/schema": "^6.114.2",
    "@tsed/swagger": "^6.114.2",
    "ajv": "^8.11.0",
    "barrelsby": "^2.3.4",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.3",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mysql2": "^2.3.3",
    "passport": "0.4.1",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "3.20.9",
    "@tsed/cli-plugin-jest": "3.20.9",
    "@tsed/cli-plugin-typeorm": "3.20.9",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.30",
    "@types/passport": "^1.0.7",
    "@types/passport-http": "^0.3.9",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^28.0.2",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "tsed": {
    "packageManager": "yarn",
    "convention": "default",
    "architecture": "default"
  }
}
